library(MASS)
library(dplyr)
Vans93 <- Cars93 %>%
filter(Type == "Van") %>%
dplyr::select(Model, Price, EngineSize, Horsepower, MPG.highway, Length)
Vans93
Vans_Euclidean <- dist(as.matrix(Vans93[, -1]), method = "euclidean")
Vans_Euclidean_matrix <- as.matrix(Vans_Euclidean)
rownames(Vans_Euclidean_matrix) <- Vans93$Model
colnames(Vans_Euclidean_matrix) <- Vans93$Model
Vans_Euclidean_matrix
Vans_Euclidean_rounded <- round(Vans_Euclidean_matrix, 2)
Vans_Euclidean_rounded
